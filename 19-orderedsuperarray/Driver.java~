public class Driver{
    public static void main(String[] args) {
	try{
	    sarray s = new sarray();
	    
	    System.out.println(s.toString());
	    System.out.println(s.size());
	    for (int i=0;i<10;i++)  {
		s.add(i,"JEANNEEEE");
	    }
	    s.set(5,"DERRICK");
	    System.out.println(s.toString());
	    System.out.println(s.size());
	    
	} catch (ArrayIndexOutOfBoundsException e) {
	    System.out.println("An error has occured: "+e);
	}
    
    }
	
	
}
